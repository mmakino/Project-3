{
  "name": "project-3",
  "version": "1.0.0",
  "description": "Bar inventory app",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "gravatar": "^1.8.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react-bootstrap": "^1.0.0-beta.6",
    "sequelize": "^5.1.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' server\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenaym/Project-3.git"
  },
  "author": "birds of fright",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jenaym/Project-3/issues"
  },
  "homepage": "https://github.com/jenaym/Project-3#readme"
}
